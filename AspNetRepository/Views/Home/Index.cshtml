

@model List<Employee>

<table class="table table-bordered">
    <tbody>
        @foreach (var item in Model)
        {
            
            <tr>
                <td>@item.Id</td>
                <td>@item.name</td>
                <td>@item.family</td>
                <td>@item.salary</td>
            </tr>
        }
    </tbody>
</table>